generator client {
  provider = "prisma-client-js"
}

datasource db1 {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model aac_user {
  id         Int      @id @default(autoincrement())
  userCode   String   @unique
  userName   String
  password   String?
  salt       String?
  email      String
  mobile     String?
  telephone  String?
  remark     String?
  deleted    Boolean  @default(false)
  createdaAt DateTime @default(now())
  updatedAt  DateTime @updatedAt
  deletedAt  DateTime
}
